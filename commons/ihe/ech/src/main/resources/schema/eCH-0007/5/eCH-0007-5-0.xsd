<xs:schema xmlns:eCH-0007="http://www.ech.ch/xmlns/eCH-0007/5" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.ech.ch/xmlns/eCH-0007/5" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.0">
